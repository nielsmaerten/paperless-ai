{
  "name": "paperless-ai",
  "version": "1.0.0",
  "description": "AI based Tag, correspondent and meta data generation",
  "main": "server.js",
  "scripts": {
    "test": "nodemon server.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check **/*.{js,json,md,css}",
    "format": "prettier --write **/*.{js,json,md,css}",
    "fix": "npm run format && npm run lint:fix"
  },
  "keywords": [
    "paperless-ngx",
    "ai",
    "paperless",
    "artificial",
    "intelligence"
  ],
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "node_modules/*",
      ".git/*",
      "*.log",
      "public/*",
      "views/*",
      "document.json",
      "OPENAPI/openapi.json"
    ]
  },
  "author": "Clusterzx",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.2",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.8.1",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dockerode": "^4.0.6",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.9",
    "openai": "^4.86.2",
    "rimraf": "^6.0.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tiktoken": "^1.0.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "globals": "^16.0.0",
    "prettier": "^3.5.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@scarf/scarf",
      "better-sqlite3"
    ]
  }
}
